{
  "name": "@gabemule/react-boilerplate",
  "version": "1.0.0",
  "description": "A Custom React Boilerplate",
  "main": "dist/lib/main.js",
  "scripts": {
    "build:app": "webpack --config .webpack/app.prod.js",
    "build:lib": "webpack --config .webpack/lib.prod.js",
    "dev:app": "webpack-dev-server --open --hot --config .webpack/app.dev.js",
    "dev:lib": "webpack --config .webpack/lib.dev.js --watch --progress",
    "lint": "eslint src --fix",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "author": "Gabriel Mule <gabemule@gmail.com>",
  "repository": "https://github.com/gabemule/react-boilerplate",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "@gabemule/react-boilerplate": "file:./"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "autoprefixer": "^10.4.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
